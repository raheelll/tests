SELECT v3_option_set.* ,(SELECT site_id FROM v3_site WHERE batch_id = 'DH3eRyfSPlaW4J8' GROUP BY site_id ) as site_id FROM v3_option_set WHERE option_set_serial_id in (SELECT max(option_set_serial_id) as option_set_serial_id from v3_option_set join v3_rel_product_option_set AS pos on pos.option_set_id=v3_option_set.option_set_id join v3_product AS p on p.product_id=pos.product_id join v3_site on v3_option_set.company_id=v3_site.company_id where v3_option_set.is_firebase =1 and v3_option_set.show_online=1 and v3_option_set.batch_id = 'DH3eRyfSPlaW4J8' and v3_site.batch_id = 'DH3eRyfSPlaW4J8' and v3_option_set.is_profanity=0 and p.is_profanity=1 GROUP BY v3_option_set.option_set_id)



SELECT v3_product.product_id,v3_option_set.batch_id,v3_product.name,v3_rel_product_option_set.option_set_id as OPTIONSETID, (SELECT name as optionSetName FROM v3_option_set WHERE v3_option_set.is_profinity = 0 AND option_set_id = OPTIONSETID LIMIT 1) as optionSetName FROM v3_product LEFT JOIN v3_rel_product_option_set ON v3_rel_product_option_set.product_id = v3_product.product_id LEFT JOIN v3_option_set ON v3_option_set.option_set_id = v3_rel_product_option_set.option_set_id WHERE v3_product.is_profinity = 0 GROUP BY v3_product.product_id
